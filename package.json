{
  "name": "coypu",
  "version": "1.1.0",
  "homepage": "https://coypu.co",
  "license": "CC-BY-ND-4.0",
  "description": "Text-editor-like week planner",
  "author": "Lost Bananas <hello@lostbananas.com>",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "pack": "npm run build && build --dir",
    "dist": "npm run build && build -mwl",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "build": {
    "appId": "bkzl.coypu",
    "copyright": "© 2017 Bartłomiej Kozal, Maciej Kozal",
    "productName": "Coypu",
    "asar": true,
    "forceCodeSigning": true,
    "publish": {
      "provider": "generic",
      "url": "https://download.coypu.co/"
    },
    "dmg": {
      "title": "${productName}"
    },
    "directories": {
      "buildResources": "build/resources",
      "output": "dist",
      "app": "app"
    },
    "linux": {
      "category": "Office",
      "packageCategory": "misc",
      "target": "deb",
      "synopsis": "Text-editor-like week planner"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "squirrelWindows": {
      "iconUrl": "https://coypu.co/icon.ico",
      "loadingGif": "build/resources/spinner.gif"
    },
    "win": {
      "target": "squirrel",
      "certificateFile": "/Users/bkzl/.ssl/cert.p12",
      "certificatePassword": "lac25couture"
    }
  },
  "postinstall": "install-app-deps",
  "dependencies": {
    "autosize": "^3.0.20",
    "lodash": "^4.17.3",
    "moment": "^2.17.1",
    "moment-range": "^2.2.0",
    "normalize.css": "^5.0.0",
    "pouchdb": "^6.1.1",
    "vue": "^2.1.0",
    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chromedriver": "^2.21.2",
    "connect-history-api-fallback": "^1.1.0",
    "cross-spawn": "^4.0.2",
    "css-loader": "^0.25.0",
    "electron": "^1.4.13",
    "electron-builder": "^11.4.4",
    "electron-builder-squirrel-windows": "^11.4.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "function-bind": "^1.0.2",
    "html-webpack-plugin": "^2.8.1",
    "http-proxy-middleware": "^0.17.2",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lolex": "^1.4.0",
    "mocha": "^3.1.0",
    "nightwatch": "^0.9.8",
    "opn": "^4.0.2",
    "ora": "^0.3.0",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "selenium-server": "2.53.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.4",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
